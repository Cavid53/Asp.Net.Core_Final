@model HomeModel
@{
    ViewData["Title"] = "Index";
}



<!--Slider Area Start-->
<div class="slider-area">
    <div class="hero-slider owl-carousel">
        <!--Single Slider Start-->
        @foreach (var slider in Model.Sliders)
        {
            <div class="single-slider" style="background-image: url(img/slider/@slider.Image)">
                <div class="hero-slider-content">
                    <h1>@slider.Description</h1>
                    <h2>@slider.Title</h2>
                    <div class="slider-btn">
                        <a href="#">Shop Now</a>
                    </div>
                </div>
            </div>
        }

        <!--Single Slider End-->
    </div>
</div>
<!--Slider Area End-->
<!--Feature Area Start-->
<div class="feature-area pt-80 pb-50">
    <div class="container">
        <div class="row">
            @foreach (var single in Model.SingleFeatures)
            {
                <div class="col-lg-4 col-md-6">
                    <!--Single Feature Area Start-->
                    <div class="single-feature-area mb-30">
                        <div class="feature-content">
                            <a href="#" style="font-size:25px;color:goldenrod;margin-left:20px;"><i class="@single.IconField"></i></a>
                            <h3 class="iconBefore">@single.Title</h3>


                        </div>
                    </div>
                    <!--Single Feature Area End-->
                </div>
            }

        </div>
    </div>



</div>
<!--Feature Area End-->
<!--Our Collection Area Start-->
<div class="our-collection-area collection-bg pb-75" style="background-image: url(img/bg/@Model.BackgroundImages.ElementAt(0).Image);">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <!--Section Title Start-->
                <div class="section-title">
                    <span class="sub-title">@Model.RepeatTitleDescs.ElementAt(0).Title</span>
                    <h2>@Model.RepeatTitleDescs.ElementAt(0).Description</h2>
                </div>
                <!--Section Title End-->
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                <!--Single Categorie Collection Start-->
                <div class="single-categorie-collection mt-30">
                    <div class="categorie-collection-img">
                        <a href="#"><img src="~/img/banner/@Model.SingleCategories.ElementAt(0).Image" alt=""></a>
                    </div>
                </div>
                <!--Single Categorie Collection End-->
            </div>
            <div class="col-md-3">
